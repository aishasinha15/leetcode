class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        int l=nums.size();
        vector<int> C;
        int i=0,j=n;
        while(i<n && j<l){
            C.push_back(nums[i]);
            C.push_back(nums[j]);
            i++;
            j++;
        }
        return C;
    }
};
